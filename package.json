{
  "name": "qwik-docs",
  "version": "0.0.1",
  "description": "Qwik Docs Site",
  "homepage": "https://qwik.builder.io/",
  "main": "src/main.tsx",
  "scripts": {
    "build": "yarn build.client && yarn build.static && yarn ssg",
    "build.client": "vite build",
    "build.static": "vite build --ssr src/entry.static.tsx",
    "dev": "node --inspect ../../node_modules/vite/bin/vite.js --force",
    "dev.ssr": "node --inspect ../../node_modules/vite/bin/vite.js --mode ssr --force --open",
    "dev.debug": "node --inspect-brk ../../node_modules/vite/bin/vite.js --mode ssr --force",
    "serve": "wrangler pages dev ./dist",
    "ssg": "node server/entry.static",
    "start": "yarn dev.ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "preview": "qwik build preview && vite preview --open",
    "contributors": "tsm contributors.ts"
  },
  "devDependencies": {
    "@algolia/autocomplete-core": "^1.7.1",
    "@algolia/client-search": "^4.14.2",
    "@builder.io/partytown": "^0.7.1",
    "@builder.io/qwik": "0.10.0",
    "@builder.io/qwik-city": "0.0.112",
    "@builder.io/sdk-qwik": "^0.0.26",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@docsearch/css": "^3.2.1",
    "algoliasearch": "^4.14.2",
    "autoprefixer": "10.4.12",
    "fflate": "^0.7.4",
    "gray-matter": "^4.0.3",
    "node-fetch": "3.2.10",
    "postcss": "8.4.17",
    "prism-themes": "1.9.0",
    "prismjs": "^1.29.0",
    "tailwindcss": "3.1.8",
    "tsm": "^2.2.2",
    "typescript": "4.8.4",
    "uvu": "0.5.6",
    "vite": "3.1.7",
    "wrangler": "beta"
  },
  "author": "Builder.io Team",
  "bugs": {
    "url": "https://github.com/BuilderIO/qwik"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "private": true
}
